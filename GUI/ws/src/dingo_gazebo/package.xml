<?xml version="1.0"?>
<package format="2">
  <name>dingo_gazebo</name>
  <version>0.1.2</version>
  <description>Launchfiles to use Dingo in Gazebo.</description>

  <maintainer email="tbaltovski@clearpathrobotics.com">Tony Baltovski</maintainer>
  <author>Tony Baltovski</author>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <exec_depend>dingo_control</exec_depend>
  <exec_depend>dingo_description</exec_depend>
  <exec_depend>gazebo_plugins</exec_depend>
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>gazebo_ros_control</exec_depend>
  <exec_depend>hector_gazebo_plugins</exec_depend>
  <exec_depend>ridgeback_gazebo_plugins</exec_depend>
  <test_depend>roslaunch</test_depend>

  <export>
    <gazebo_ros gazebo_media_path="${prefix}"/>
  </export>
</package>
